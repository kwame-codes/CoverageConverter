{
    "manifestVersion": 1,
    "id": "build-release-task",
    "name": "Coverage Converter (mod)",
    "version": "0.1.26",
    "publisher": "kwame",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Convert .coverage file into .coveragexml for use in Code Coverage Report",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/engine-outline512x512.png"
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "tags": [
        "devops",
        "code coverage",
        "convert .coverage",
        "coverage",
        "report",
        "vstest",
        "mstest"
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/kwame-codes/CoverageConverter"
    },
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ],
    "galleryFlags": [
        "Public"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    }
}